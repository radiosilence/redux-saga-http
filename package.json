    "fetch-mock": "6.0.1",
    "html-webpack-plugin": "3.0.6",
    "isomorphic-fetch": "2.2.1",
    "jest": "22.4.2",
    "lite-server": "^2.3.0",
    "morgan": "1.9.0",
